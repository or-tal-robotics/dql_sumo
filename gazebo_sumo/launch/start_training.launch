<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find gazebo_sumo)/config/sumo_params.yaml" />
    <!-- Launch the training system -->
    <node pkg="gazebo_sumo" name="turtlebot2_sumo" type="start_training.py" output="screen"/>
</launch>
